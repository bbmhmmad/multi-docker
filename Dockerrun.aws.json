{
    "AWSEBDockerrunVersion":2,
    "conatinerDefinitions": [
        {
            "name":"client",
            "image": "bbmhmmad/multi-client",
            "hostname":"client",
            "essential" : false
        },
        {
            "name":"server",
            "image": "bbmhmmad/multi-server",
            "hostname":"api",
            "essential" : false
        },
        {
            "name":"worker",
            "image": "bbmhmmad/multi-worker",
            "hostname":"worker",
            "essential" : false
        },
        {
            "name":"nginx",
            "image": "bbmhmmad/multi-nginx",
            "essential" : true,
            "portMappings": [
                {
                    "host": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}